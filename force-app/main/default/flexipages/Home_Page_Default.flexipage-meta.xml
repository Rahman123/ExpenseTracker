            </componentInstanceProperties>
            <componentInstanceProperties>
                <name>maxRecords</name>
                <value>3</value>
            </componentInstanceProperties>
            <componentName>flexipage:recentItems</componentName>
            <visibilityRule>
                <criteria>
                    <leftValue>{!$Permission.StandardPermission.ManageUsers}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>false</rightValue>
                </criteria>
            </visibilityRule>
        </componentInstances>
        <name>right</name>
        <type>Region</type>
